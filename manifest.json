{
    "name": "YT MangaReader",
    "version": "1.0.0",
    "description": "Read your favorites Mangas in Youtube",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/watch?*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_title": "YT MangaReader"
    },
    "web_accessible_resources": ["script.js"]
}